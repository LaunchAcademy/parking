<%= form_for @parking_registration do |f| %>
  <div>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
  </div>

  <div>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
  </div>

  <div>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>

  <div>
    <%= f.label :spot_number %>
    <%= f.number_field :spot_number %>
  </div>

  <%= f.submit 'Register' %>
<%- end -%>
